<div class="layout-search-list">
    <div class="container">
        <section>
            <h1>東京都23区のお店一覧</h1>

            <% @tokyo1.each do |s| %>
            <div class="layout-listitem">
                <div class="row">
                    <div class="col-sm-2"><%= s.shopname %></div>
                    <div class="col-sm-2"><%= link_to s.branchname, s %></div>
                    <div class="col-sm-6"><%= s.address %></div>
                    <div class="col-sm-2"><%= link_to "店舗詳細", s ,class: "btn btn-default btn-sm" %></div>
                </div>
                <!-- .row -->
            </div>
            <% end %>

        </section>
    </div>
    <!-- container -->
</div>
<!-- layout-search-list -->
