<div class="container ">
    <h3>支店編集フォーム</h3>
    <div class="col-md-6 col-md-offset-3">
        <%= form_for (@shopbranch) do |f| %>
        <%= render 'shared/error_messages_branch' %>

        <div class="form-grop">
            <%= f.label :shopinfo_id %>
            <%= f.text_field :shopinfo_id, class: 'form-control' %>
        </div>
        <div class="form-grop">
            <%= f.label :shopname %>
            <%= f.text_field :shopname, class: 'form-control' %>
        </div>
        <div class="form-grop">
            <%= f.label :branchname %>
            <%= f.text_field :branchname, class: 'form-control' %>
        </div>
        <div class="form-grop">
            <%= f.label :address %>
            <%= f.text_field :address, class: 'form-control' %>
        </div>
        <div class="form-grop">
            <%= f.label :tel %>
            <%= f.text_field :tel, class: 'form-control' %>
        </div>
        <div class="form-grop">
            <%= f.label :starthour %>
            <%= f.text_field :starthour, class: 'form-control' %>
        </div>
        <div class="form-grop">
            <%= f.label :endhour %>
            <%= f.text_field :endhour, class: 'form-control' %>
        </div>
        <div class="form-grop">
            <%= f.label :e_starthour %>
            <%= f.text_field :e_starthour, class: 'form-control' %>
        </div>
        <div class="form-grop">
            <%= f.label :e_endhour %>
            <%= f.text_field :e_endhour, class: 'form-control' %>
        </div>
        <div class="form-grop">
            <%= f.label :holiday %>
            <%= f.text_field :holiday, class: 'form-control' %>
        </div>
        <div class="form-grop">
            <%= f.label :latitude %>
            <%= f.number_field :latitude, class: 'form-control' %>
        </div>
        <div class="form-grop">
            <%= f.label :longitude %>
            <%= f.number_field :longitude, class: 'form-control' %>
        </div>
        <div class="form-grop">
            <%= f.label :train %>
            <%= f.text_field :train, class: 'form-control' %>
        </div>
        <div class="form-grop">
            <%= f.label :station %>
            <%= f.text_field :station, class: 'form-control' %>
        </div>
        <div class="form-grop">
            <%= f.label :train2 %>
            <%= f.text_field :train2, class: 'form-control' %>
        </div>
        <div class="form-grop">
            <%= f.label :station2 %>
            <%= f.text_field :station2, class: 'form-control' %>
        </div>
        <div class="form-grop">
            <%= f.label :train3 %>
            <%= f.text_field :train3, class: 'form-control' %>
        </div>
        <div class="form-grop">
            <%= f.label :station3 %>
            <%= f.text_field :station3, class: 'form-control' %>
        </div>

        <div class="form-grop">
            <%= f.label :area %>
            <%= f.text_field :area, class: 'form-control' %>
        </div>
        <div class="form-grop">
            <%= f.label :prefec %>
            <%= f.text_field :prefec, class: 'form-control' %>
        </div>
        <div class="form-grop">
            <%= f.label :city %>
            <%= f.text_field :city, class: 'form-control' %>
        </div>
        <div class="form-grop">
            <%= f.label :access %>
            <%= f.text_area :access, class: 'form-control', rows: "7"%>
        </div>
        <div class="form-grop">
            <%= f.label :sheet %>
            <%= f.text_field :sheet, class: 'form-control' %>
        </div>
        <div class="form-grop">
            <%= f.label :info %>
            <%= f.text_area :info, class: 'form-control', rows: "7" %>
        </div>
        <%= f.submit "情報更新", class: "btn btn-primary" %>
        <% end %>
    </div>
</div>
